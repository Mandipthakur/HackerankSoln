if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    s=0
    avg_mark=float()
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    avr = sum(student_marks[query_name])/len(student_marks[name])  
    print("%.2f"%avr) 
